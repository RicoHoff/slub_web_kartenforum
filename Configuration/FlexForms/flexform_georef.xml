<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Configuration of Plugin Georeference.</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.test>
                        <TCEforms>
                            <label>Test</label>
                            <config>
                                <type>input</type>
                                <size>128</size>
                            </config>
                        </TCEforms>
                    </settings.test>
                    <settings.flexform.georefBackend>
                        <TCEforms>
                            <label>URL of the georeferencing backend</label>
                            <config>
                                <type>input</type>
                                <size>128</size>
                            </config>
                        </TCEforms>
                    </settings.flexform.georefBackend>
                    <settings.flexform.georefAuthUser>
                        <TCEforms>
                            <label>Basic Auth - Username</label>
                            <config>
                                <type>input</type>
                                <size>64</size>
                            </config>
                        </TCEforms>
                    </settings.flexform.georefAuthUser>
                    <settings.flexform.gerefAuthPassword>
                        <TCEforms>
                            <label>Basic Auth - Password</label>
                            <config>
                                <type>input</type>
                                <size>64</size>
                            </config>
                        </TCEforms>
                    </settings.flexform.gerefAuthPassword>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
