plugin.tx_slubwebkartenforum {
	features {
	    requireCHashArgumentForActionArguments = 0
	}
	persistence {
		storagePid = {$config.userStoragePid}
		classes {
			Slub\SlubWebKartenforum\Domain\Model\User {
				mapping {
					tableName = fe_users
					columns {
						name.mapOnProperty = name
					}
				}
			}
		}
	}
}
